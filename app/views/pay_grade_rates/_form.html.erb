<%= simple_form_for(@pay_grade_rate) do |f| %>
  <%= f.error_notification %>
  <div class="row">
    <div class="form-inputs">
      <%= f.input :pay_grade_id, label_html: { class: 'active' } %>
      <%= f.input :rate, label_html: { class: 'active' } %>
      <%= f.input :start_date, label_html: { class: 'active' } %>
      <%= f.input :end_date, label_html: { class: 'active' } %>
    </div>
  </div>
  <div class="row">
    <div class="form-actions col s8">
      <%= f.button :submit %>
    </div>
  </div>
<% end %>